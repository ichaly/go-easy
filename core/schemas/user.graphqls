directive @goModel(
    model: String
    models: [String!]
) on OBJECT | INPUT_OBJECT | SCALAR | ENUM | INTERFACE | UNION

directive @goField(
    forceResolver: Boolean
    name: String
) on INPUT_FIELD_DEFINITION | FIELD_DEFINITION

directive @goTag(
    key: String!
    value: String
) on INPUT_FIELD_DEFINITION | FIELD_DEFINITION

type User {
    id: ID!
    name: String!
}

input NewUser {
    id: ID!
    name: String!
}


type Query {
    users: [User!]!
}

type Mutation {
    createUser(input: NewUser!): User!
}